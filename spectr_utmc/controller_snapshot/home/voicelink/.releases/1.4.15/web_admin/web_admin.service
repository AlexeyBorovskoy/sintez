[Unit]
Description=Gunicorn instance to serve web_admin
After=network.target

[Service]
User=voicelink
Group=www-data
WorkingDirectory=/home/voicelink/rtc/web_admin
ExecStart=/usr/bin/gunicorn --workers 1 --bind unix:web_admin.sock -m 007 --timeout 300 wsgi:app

[Install]
WantedBy=multi-user.target
